package com.jp.hr.entities;

public class Employee {
	private int empID;
	private String firstName;
	private String lastName;
	
	public Employee(int empID, String firstName, String lastName) {
		
		this.empID = empID;
		this.firstName = firstName;
		this.lastName = lastName;
	}

	public Employee() {
		super();
	}
	
	

}
